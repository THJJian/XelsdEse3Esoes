@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    @foreach (var css in WorkContext.Resource.Css)
    {
        <link href="@css" type="text/css" rel="stylesheet">
    }
    <style>
        html body {
            margin: 0;
            padding: 0;
        }
        .main {
            width: 100%;
            height: 100%;
        }
    </style>
</head>
<body class="easyui-layout">

    <div data-options="region:'north', minWidth:'900px'" style="height:100px;"></div>
    <div data-options="region:'south', minWidth:'900px'" style="height:100px;"></div>
    <div data-options="region:'west', minHeight:'768px'" style="width:100px;"></div>
    @*<div data-options="region:'east'" style="width:100px; "></div>*@
    <div data-options="region:'center', minHeight:'668px', minWidth:'800px'" style="padding: 5px;">
        <div style="margin: 0; padding: 0; min-height: 668px; min-width: 800px; background: red;"></div>
    </div>
    

    @foreach (var script in WorkContext.Resource.JavaScript)
    {
        <script type="text/javascript" src="@script"></script>
    }
</body>
</html>
